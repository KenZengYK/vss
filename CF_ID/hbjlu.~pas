uses hbbgu, idcxu, impqcdm;
{$R *.DFM}

procedure Tfrmhbjl.BitBtn4Click(Sender: TObject);
begin
  close;
end;

procedure Tfrmhbjl.BitBtn3Click(Sender: TObject);
begin
  if frmidcx=nil then frmidcx:=tfrmidcx.create(self);
  frmidcx.MaskEdit1.text:='      -S    ';
  frmidcx.label2.caption:='yhb';   
  frmidcx.show;
end;

procedure Tfrmhbjl.FormShow(Sender: TObject);
begin
  (table2.fieldbyname('jbshl') as tfloatfield).displayformat:='#####0.00';
  (table2.fieldbyname('fsshl') as tfloatfield).displayformat:='#####0.00';
  (table2.fieldbyname('shl') as tfloatfield).displayformat:='#####0.00';
  (table2.fieldbyname('jcsh') as tfloatfield).displayformat:='#####0.00';
end;

end.

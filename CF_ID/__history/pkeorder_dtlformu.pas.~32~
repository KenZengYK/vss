    procedure ppReport6PreviewFormCreate(Sender: TObject);
    procedure ppHeaderBand6BeforePrint(Sender: TObject);
    procedure ppGroupFooterBand1BeforePrint(Sender: TObject);
    procedure ppSummaryBand1BeforePrint(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  frmpkeorder_dtl: Tfrmpkeorder_dtl;

implementation

uses mainu, eOrderFormu;

{$R *.dfm}

procedure Tfrmpkeorder_dtl.ppGroupFooterBand1BeforePrint(Sender: TObject);
begin
      with ADOQuery11 do begin
        close;
        //Parameters.Clear;
        sql.Text:='select * from tmp_pkeorder_dtls where dt=:x1 and prjno=:x2 order by seq';
        Parameters[0].Value:=ADOQuery9.FieldByName('dt').Value;
        Parameters[1].Value:=ADOQuery9.FieldByName('prjno').Value;
        //ShowMessage('2');
        Open;
      end;
end;

procedure Tfrmpkeorder_dtl.ppHeaderBand6BeforePrint(Sender: TObject);
begin
  with ADOQuery4 do begin
    close;
    //Parameters.clear;
    sql.Text:='select * from tmp_pkeorder where dt=:x1 and prjno=:x2';
    Parameters[0].Value:=ADOQuery9.FieldByName('dt').Value;
    Parameters[1].Value:=ADOQuery9.FieldByName('prjno').Value;
    open;
    if not FieldByName('gch').IsNull then prj001.Caption:=FieldByName('gch').Value
    else prj001.Caption:='';
    if not FieldByName('swsdd').IsNull then fstdd001.Caption:=FormatDateTime('yyyy/MM/dd',FieldByName('swsdd').Value)
    else fstdd001.Caption:='';
    if not FieldByName('ttl_marker').IsNull then marker001.Caption:=FieldByName('ttl_marker').AsString
    else marker001.Caption:='';
  end;
end;

procedure Tfrmpkeorder_dtl.ppReport6PreviewFormCreate(Sender: TObject);
begin
  ppreport6.PreviewForm.WindowState:=wsmaximized;
  tppviewer(ppreport6.previewform.viewer).zoompercentage:=100;
end;

procedure Tfrmpkeorder_dtl.ppSummaryBand1BeforePrint(Sender: TObject);
var
  i:integer;
begin
  ttlm001.Caption:=frmeOrder.ADOQuery8.FieldByName('ttl_wl').AsString;
  i:=0;
  with ADOQuery6 do begin
    close;
    SQL.Text:='select * from tmp_pkeorder_dtlm where dt=:x1 and prjno=:x2 order by wl';
    Parameters[0].Value:=ADOQuery9.FieldByName('dt').Value;
    Parameters[1].Value:=ADOQuery9.FieldByName('prjno').Value;
    Open;
    first;
    while not Eof do begin
      i:=i+1;
      if i=1 then begin
        if not FieldByName('wl').IsNull then
        wl001.Caption:=Copy(FieldByName('wl').Value,5,20)
        else wl001.Caption:='';
        mk001.Caption:=FieldByName('marker_ttl').AsString;
      end else if i=2 then begin
        if not FieldByName('wl').IsNull then
        wl002.Caption:=Copy(FieldByName('wl').Value,5,20)
        else wl002.Caption:='';
        mk002.Caption:=FieldByName('marker_ttl').AsString;
      end else if i=3 then begin
        if not FieldByName('wl').IsNull then
        wl003.Caption:=Copy(FieldByName('wl').Value,5,20)
        else wl003.Caption:='';
        mk003.Caption:=FieldByName('marker_ttl').AsString;
      end else if i=4 then begin
        if not FieldByName('wl').IsNull then
        wl004.Caption:=Copy(FieldByName('wl').Value,5,20)
        else wl004.Caption:='';
        mk004.Caption:=FieldByName('marker_ttl').AsString;
      end else if i=5 then begin
        if not FieldByName('wl').IsNull then
        wl005.Caption:=Copy(FieldByName('wl').Value,5,20)
        else wl005.Caption:='';
        mk005.Caption:=FieldByName('marker_ttl').AsString;
      end else if i=6 then begin
        if not FieldByName('wl').IsNull then
        wl006.Caption:=Copy(FieldByName('wl').Value,5,20)
        else wl006.Caption:='';
        mk006.Caption:=FieldByName('marker_ttl').AsString;
      end else if i=7 then begin
        if not FieldByName('wl').IsNull then
        wl007.Caption:=Copy(FieldByName('wl').Value,5,20)
        else wl007.Caption:='';
        mk007.Caption:=FieldByName('marker_ttl').AsString;
      end else if i=8 then begin
        if not FieldByName('wl').IsNull then
        wl008.Caption:=Copy(FieldByName('wl').Value,5,20)
        else wl008.Caption:='';
        mk008.Caption:=FieldByName('marker_ttl').AsString;
      end;
      Application.ProcessMessages;
      next;
    end;
  end;
end;

end.

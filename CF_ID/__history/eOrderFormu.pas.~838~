procedure TfrmeOrder.ppGroupFooterBand14BeforePrint(Sender: TObject);
var
  splice:double;
  i1:Integer;
  lys:string;
begin
  splice:=0;
  splice:=adoquery1.FieldByName('splice').Value;
  yield01m.caption:='**Laying usage excl. Def. Relaxation% b4 cutting on Style type, incl. '+formatfloat('#0.000',splice)+' m of splice usage ttl. ';

  mks001.Caption:=ADOQuery1.FieldByName('ttl_xh').AsString;
  with ADOQuery12 do begin
    Close;
    SQL.Text:='select count(distinct kh) as x1 from cut_fcjy3 where prjno=:x1 and wl=:x2';
    Parameters[0].Value:=adoquery1.FieldByName('prjno').Value;
    Parameters[1].Value:=adoquery1.FieldByName('wl').Value;
    open;
    if not FieldByName('x1').IsNull then st001.Caption:=FieldByName('x1').AsString
    else st001.Caption:='';
  end;

  i1:=0;
  lys:='';
  with ADOQuery12 do begin
    Close;
    SQL.Text:='select distinct rtrim(ltrim(colorgroup)) as colorgroup from cut_ido_br where prjno=:x1 and matl=:x2 and colorgroup>''''';
    Parameters[0].Value:=adoquery1.FieldByName('prjno').Value;
    Parameters[1].Value:=adoquery1.FieldByName('wl_n').Value;
    open;
    First;
    while not eof do begin
      i1:=i1+1;
      if not FieldByName('colorgroup').IsNull then begin
        if lys>'' then lys:=lys+','+FieldByName('colorgroup').Value
        else lys:=FieldByName('colorgroup').Value;
      end;
      Application.ProcessMessages;
      Next;
    end;
  end;
  if i1>0 then begin
    lst001.Caption:=IntToStr(i1);
    lstc001.Caption:=lys;
  end else begin
    lst001.Caption:='';
    lstc001.Caption:='';
  end;
end;

procedure TfrmeOrder.ppGroupFooterBand17BeforePrint(Sender: TObject);
begin
  if dif33_01.FieldValue>0 then begin
    dif33_01.Font.Color:=clRed;
    difp33_01.Font.Color:=clRed;
  end else begin
    dif33_01.Font.Color:=clBlack;
    difp33_01.Font.Color:=clBlack;
  end;
  if dif31_01.FieldValue>0 then begin
    dif31_01.Font.Color:=clRed;
    difp31_01.Font.Color:=clRed;
  end else begin
    dif31_01.Font.Color:=clBlack;
    difp31_01.Font.Color:=clBlack;
  end;
  if dif22_01.FieldValue>0 then begin
    dif22_01.Font.Color:=clRed;
    difp22_01.Font.Color:=clRed;
  end else begin
    dif22_01.Font.Color:=clBlack;
    difp22_01.Font.Color:=clBlack;
  end;
  if ADOQuery14.FieldByName('wl_same').Value='0' then
    txtwl01.Font.Color:=clRed
  else txtwl01.Font.Color:=clBlack;
end;

procedure TfrmeOrder.ppGroupFooterBand1BeforePrint(Sender: TObject);
begin
  if not ADOQuery1.FieldByName('type21').IsNull then begin
    if ADOQuery1.FieldByName('type21').Value='a' then begin
      t2101.Visible:=true;
      t2102.Visible:=false;
      t2103.Visible:=false;
    end else if adoquery1.FieldByName('type21').Value='b' then begin
      t2101.Visible:=False;
      t2102.Visible:=True;
      t2103.Visible:=false;
    end else if adoquery1.FieldByName('type21').Value='c' then begin
      t2101.Visible:=False;
      t2102.Visible:=false;
      t2103.Visible:=True;
    end else begin
      t2101.Visible:=False;
      t2102.Visible:=false;
      t2103.Visible:=false;
    end;
  end else begin
    t2101.Visible:=False;
    t2102.Visible:=false;
    t2103.Visible:=false;
  end;

  if not ADOQuery1.FieldByName('type31').IsNull then begin
    if ADOQuery1.FieldByName('type31').Value='a' then begin
      t3101.Visible:=true;
      t3102.Visible:=false;
      t3103.Visible:=false;
      t3104.Visible:=false;
    end else if adoquery1.FieldByName('type31').Value='b' then begin
      t3101.Visible:=False;
      t3102.Visible:=True;
      t3103.Visible:=false;
      t3104.Visible:=false;
    end else if adoquery1.FieldByName('type31').Value='c' then begin
      t3101.Visible:=False;
      t3102.Visible:=false;
      t3103.Visible:=True;
      t3104.Visible:=false;
    end else if adoquery1.FieldByName('type31').Value='d' then begin
      t3101.Visible:=False;
      t3102.Visible:=false;
      t3103.Visible:=False;
      t3104.Visible:=True;
    end else begin
      t3101.Visible:=False;
      t3102.Visible:=false;
      t3103.Visible:=false;
      t3104.Visible:=false;
    end;
  end else begin
    t3101.Visible:=False;
    t3102.Visible:=false;
    t3103.Visible:=false;
    t3104.Visible:=false;
  end;
end;

procedure TfrmeOrder.ppGroupFooterBand20BeforePrint(Sender: TObject);
begin
  if dif31_02.FieldValue>0 then begin
    dif31_02.Font.Color:=clRed;
    difp31_02.Font.Color:=clRed;
  end else begin
    dif31_02.Font.Color:=clBlack;
    difp31_02.Font.Color:=clBlack;
  end;
  if dif22_02.FieldValue>0 then begin
    dif22_02.Font.Color:=clRed;
    difp22_02.Font.Color:=clRed;
  end else begin
    dif22_02.Font.Color:=clBlack;
    difp22_02.Font.Color:=clBlack;
  end;
  if ADOQuery14.FieldByName('wl_same').Value='0' then
    txtwl02.Font.Color:=clRed
  else txtwl02.Font.Color:=clBlack;
end;


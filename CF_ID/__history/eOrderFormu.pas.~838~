procedure TfrmeOrder.ppFooterBand3BeforePrint(Sender: TObject);
begin
  if not ADOQuery7.FieldByName('type21').IsNull then begin
    if ADOQuery7.FieldByName('type21').Value='a' then begin
      y2101.Visible:=true;
      y2102.Visible:=false;
      y2103.Visible:=false;
    end else if adoquery7.FieldByName('type21').Value='b' then begin
      y2101.Visible:=False;
      y2102.Visible:=True;
      y2103.Visible:=false;
    end else if adoquery7.FieldByName('type21').Value='c' then begin
      y2101.Visible:=False;
      y2102.Visible:=false;
      y2103.Visible:=True;
    end else begin
      y2101.Visible:=False;
      y2102.Visible:=false;
      y2103.Visible:=false;
    end;
  end else begin
    y2101.Visible:=False;
    y2102.Visible:=false;
    y2103.Visible:=false;
  end;

  if not ADOQuery7.FieldByName('type31').IsNull then begin
    if ADOQuery7.FieldByName('type31').Value='a' then begin
      y3101.Visible:=true;
      y3102.Visible:=false;
      y3103.Visible:=false;
      y3104.Visible:=false;
    end else if adoquery7.FieldByName('type31').Value='b' then begin
      y3101.Visible:=False;
      y3102.Visible:=True;
      y3103.Visible:=false;
      y3104.Visible:=false;
    end else if adoquery7.FieldByName('type31').Value='c' then begin
      y3101.Visible:=False;
      y3102.Visible:=false;
      y3103.Visible:=True;
      y3104.Visible:=false;
    end else if adoquery7.FieldByName('type31').Value='d' then begin
      y3101.Visible:=False;
      y3102.Visible:=false;
      y3103.Visible:=False;
      y3104.Visible:=True;
    end else begin
      y3101.Visible:=False;
      y3102.Visible:=false;
      y3103.Visible:=false;
      y3104.Visible:=false;
    end;
  end else begin
    y3101.Visible:=False;
    y3102.Visible:=false;
    y3103.Visible:=false;
    y3104.Visible:=false;
  end;
end;


          ppLabel414.caption:='Ver 4.5 dd Sep 07 ';
          ppLabel414_0.caption:='CF Step 3.3 (CU Marker) ';
          ppReport5.Print;
        end;
      end;

      {
      with ADOQuery10 do begin
        close;
        //Parameters.Clear;
        sql.Text:='select * from tmp_pkeorder_dtl0 where dt=:x1 and prjno=:x2';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        //ShowMessage('2');
        Open;
      end;
      DataSource2.DataSet:=ADOQuery9;
      with ADOQuery9 do begin
        close;
        //Parameters.Clear;
        sql.Text:='select * from tmp_pkeorder_dtl where dt=:x1 and prjno=:x2 order by seq';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        //ShowMessage('2');
        Open;
        if not FieldByName('prjno').IsNull then begin
          //ppLabel226.caption:='Ver 1.3 dd Jun 30 ';
          ppReport6.Print;
        end;
      end;
      }

      //{
      if frmpkeorder_dtl=nil then frmpkeorder_dtl:=Tfrmpkeorder_dtl.Create(nil);
      with frmpkeorder_dtl.ADOQuery10 do begin
        close;
        //Parameters.Clear;
        sql.Text:='select * from tmp_pkeorder_dtl0 where dt=:x1 and prjno=:x2';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        //ShowMessage('2');
        Open;
      end;
      {
      with frmpkeorder_dtl.ADOQuery11 do begin
        close;
        //Parameters.Clear;
        sql.Text:='select * from tmp_pkeorder_dtls where dt=:x1 and prjno=:x2 order by seq';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        //ShowMessage('2');
        Open;
      end;
      }
      frmpkeorder_dtl.DataSource2.DataSet:=frmpkeorder_dtl.ADOQuery9;
      with frmpkeorder_dtl.ADOQuery9 do begin
        close;
        //Parameters.Clear;
        sql.Text:='select * from tmp_pkeorder_dtl where dt=:x1 and prjno=:x2 order by seq';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        //ShowMessage('2');
        Open;
        if not FieldByName('prjno').IsNull then begin
          //ppLabel226.caption:='Ver 1.3 dd Jun 30 ';
          frmpkeorder_dtl.lblstp_6.Caption:='CF Step 3.3 ';
          frmpkeorder_dtl.ppReport6.Print;
        end;
      end;
      //}




    end;
  finally
    Screen.Cursor:=crDefault;
  end;
end;


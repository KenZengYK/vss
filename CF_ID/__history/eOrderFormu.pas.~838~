procedure TfrmeOrder.SpeedButton1Click(Sender: TObject);
begin
  //if frmxzgch=nil then frmxzgch:=tfrmxzgch.create(nil);
  //frmxzgch.show;
  if frmzds=nil then frmzds:=tfrmzds.create(self);
  frmzds.caption:='d\{OX Review CF_ID';
  frmzds.label2.caption:='eOrder';
  frmzds.show;
end;


uses mainu, xzgchu, zdsu, pkeorder_dtlformu, FRN_ReviewFormu;

{$R *.dfm}

procedure TfrmeOrder.BitBtn1Click(Sender: TObject);
var
  dt:TDateTime;
  stp:string;
begin
  screen.Cursor:=crSQLWait;
  try
    if edit1.Text>'' then begin
      dt:=Now;
      if copy(edit1.text,13,2)='PU' then stp:='2.1' else stp:='3.1';
      with ADOQuery15 do begin
        close;
        SQL.Text:='exec cut_getcovering :x1,:x2,:x3';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        Parameters[2].Value:=stp;
        ExecSQL;
      end;
      //{
      with ADOQuery5 do begin
        close;
        //SQL.Text:='exec phgdb..cut_getmkeorder :x1';
        //Parameters[0].Value:=edit1.Text;
        //Parameters.Clear;
        sql.Text:='exec phgdb..cut_getmkeorder_new :x1,:x2';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        //ShowMessage('0');
        ExecSQL;
      end;
      with ADOQuery14 do begin
        close;
        sql.Text:='select * from tmp_cut_covering where dt=:x1 and prjno=:x2 order by wl';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        Open;
        if not FieldByName('dt').IsNull then begin
          if stp='2.1' then begin
            lblver004.Caption:='Ver 4.5 dd Sep 07 ';
            ppReport12.Print;
          end else if stp='3.1' then begin
            lblver002.Caption:='Ver 4.5 dd Sep 07 ';
            ppReport10.Print;
          end;
        end;
      end;

      DataSource1.DataSet:=ADOQuery7;
      with ADOQuery7 do begin
        close;
        //Parameters.Clear;
        sql.Text:='select * from tmp_mkeorder where dt=:x1 and prjno=:x2 order by wl_seq';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        //ShowMessage('1');
        Open;
        if not FieldByName('prjno').IsNull then begin
          ppLabel241.caption:='Ver 4.5 dd Sep 07 ';
          if copy(edit1.text,13,2)='PU' then
          ppLabel241_0.caption:='CF Step 2.1 (PU Marking) '
          else ppLabel241_0.caption:='CF Step 3.1 (CU Marking) ';
          ppLabel239.Caption:='Marking eOrder  ql';
          ppShape95.Visible:=false;
          ppShape96.Visible:=false;
          ppReport3.Print;
        end;
      end;

      DataSource2.DataSet:=ADOQuery3;
      with ADOQuery3 do begin
        close;
        //Parameters.Clear;
        sql.Text:='select * from tmp_mkeorder_dtl where dt=:x1 and prjno=:x2 order by wl,seq';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        //ShowMessage('2');
        Open;
        if not FieldByName('prjno').IsNull then begin
          //ppLabel226.caption:='Ver 1.3 dd Jun 30 ';
          if copy(edit1.text,13,2)='PU' then
          lblstp_4.caption:='CF Step 2.1 '
          else lblstp_4.caption:='CF Step 3.1 ';
          ppReport4.Print;
        end;
      end;
      //}
      {
      with ADOQuery1 do begin
        close;
        SQL.Text:='exec phgdb..cut_getmkeorder :x1';
        Parameters[0].Value:=edit1.Text;
        Open;
        if not FieldByName('prjno').IsNull then begin
          ppLabel226.caption:='Ver 1.2 dd Jun 29 ';
          ppReport3.Print;
        end;
      end;
      }
    end;
  finally
    Screen.Cursor:=crDefault;
  end;
end;

procedure TfrmeOrder.BitBtn2Click(Sender: TObject);
var
  ts,stp:string;
  dt:TDateTime;
begin
  screen.Cursor:=crSQLWait;
  try
    if edit1.Text>'' then begin
      ts:='';
      if Pos('-PD',Edit1.Text)=0 then ts:='Can not print Laying && Cutting eOrder for this CF_ID#';
      with ADOQuery5 do begin
        close;
        SQL.Text:='select * from cut_lbjh3 where prjno='''+edit1.Text+'''';
        open;
        if FieldByName('prjno').IsNull then ts:='Can not print Laying && Cutting eOrder for this CF_ID#';
      end;
      if ts>'' then begin
        ShowMessage(ts);
        Exit;
      end;
      dt:=Now;
      stp:='3.3';
      with ADOQuery15 do begin
        close;
        SQL.Text:='exec cut_getcovering :x1,:x2,:x3';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        Parameters[2].Value:=stp;
        ExecSQL;
      end;

      with ADOQuery14 do begin
        close;
        sql.Text:='select * from tmp_cut_covering where dt=:x1 and prjno=:x2 order by wl';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        Open;
        if not FieldByName('dt').IsNull then begin
          lblver001.Caption:='Ver 4.5 dd Sep 07 ';
          lblstp001.Caption:='CF Step 3.2 (CU Marker) ';
          ppReport9.Print;
        end;
      end;

      DataSource1.DataSet:=ADOQuery1;
      with adoquery1 do begin
        close;
        //Parameters.Clear;
        SQL.Text:='exec phgdb..cut_getlceorder :x1';
        Parameters[0].Value:=edit1.Text;
        Open;
        if not FieldByName('prjno').IsNull then begin
          ppLabel3.caption:='Ver 4.5 dd Sep 07 ';
          ppLabel3_0.caption:='CF Step 3.2 (CU Marker) ';
          ppLabel1.Caption:='Laying & Cutting eOrder  Qql';
          ppReport1.Print;
        end;
      end;
    end;
  finally
    Screen.Cursor:=crDefault;
  end;
end;

procedure TfrmeOrder.BitBtn3Click(Sender: TObject);
var
  dt:TDateTime;
  ts,stp:string;
begin
  screen.Cursor:=crSQLWait;
  try
    if edit1.Text>'' then begin
      ts:='';
      if Pos('-PD',Edit1.Text)=0 then ts:='Can not print Picking eOrder for this CF_ID#';
      with ADOQuery5 do begin
        close;
        SQL.Text:='select * from cut_cgzslr where prjno='''+edit1.Text+'''';
        open;
        if FieldByName('prjno').IsNull then ts:='Can not print Picking eOrder for this CF_ID#';
      end;
      if ts>'' then begin
        ShowMessage(ts);
        Exit;
      end;
      dt:=Now;
      {
      stp:='3.3';
      with ADOQuery15 do begin
        close;
        SQL.Text:='exec cut_getcovering :x1,:x2,:x3';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        Parameters[2].Value:=stp;
        ExecSQL;
      end;
      }
      with ADOQuery5 do begin
        close;
        //SQL.Text:='exec phgdb..cut_getmkeorder :x1';
        //Parameters[0].Value:=edit1.Text;
        //Parameters.Clear;
        sql.Text:='exec phgdb..cut_getpkeorder_new :x1,:x2';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        //ShowMessage('0');
        ExecSQL;
      end;
      {
      with ADOQuery14 do begin
        close;
        sql.Text:='select * from tmp_cut_covering where dt=:x1 and prjno=:x2 order by wl';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        Open;
        if not FieldByName('dt').IsNull then begin
          lblver001.Caption:='Ver 4.5 dd Sep 07 ';
          ppReport9.Print;
        end;
      end;
      }
      DataSource1.DataSet:=ADOQuery8;
      with ADOQuery8 do begin
        close;
        //Parameters.Clear;
        sql.Text:='select * from tmp_pkeorder where dt=:x1 and prjno=:x2 order by wl,xh';
        Parameters[0].Value:=dt;
        Parameters[1].Value:=edit1.Text;
        //ShowMessage('1');
        Open;

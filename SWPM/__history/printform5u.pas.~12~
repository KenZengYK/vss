procedure Tfrmprint5.ppReport1PreviewFormCreate(Sender: TObject);
begin
  ppreport1.previewform.windowstate:=wsmaximized;
  tppviewer(ppreport1.previewform.viewer).ZoomPercentage:=100;
end;

procedure Tfrmprint5.ppSummaryBand1BeforePrint(Sender: TObject);
begin
  with worksheet1.ClientDataSet4 do begin
    close;
    params.clear;
          commandtext:='select sum(ysjhl*zjs)/sum(zjs) as jhl0 from tblshedule where yzh=0 and ((fccs like ''%u'') or (fccs like ''%d%'') or (fccs like ''%D'')) and zjs>0';
          if worksheet1.edit2.text>'' then commandtext:=commandtext+' and upper(pline)='''+worksheet1.edit2.text+'''';
          if worksheet1.edit1.text>'' then commandtext:=commandtext+' and j_no='''+worksheet1.edit1.text+'''';
          if worksheet1.edit4.text>'' then commandtext:=commandtext+' and (tshop like '''+worksheet1.edit4.text+'%'')';
          if worksheet1.edit5.text>'' then commandtext:=commandtext+' and (cstyle like '''+worksheet1.edit5.text+'%'')';
          if worksheet1.edit6.text>'' then commandtext:=commandtext+' and tplant='''+worksheet1.edit6.text+'''';
          if worksheet1.combobox1.text>'' then commandtext:=commandtext+' and grp='''+worksheet1.combobox1.text+'''';
          if worksheet1.edit7.text>'' then commandtext:=commandtext+' and j_no like '''+worksheet1.edit7.text+'%''';
          if worksheet1.combobox2.text>'' then commandtext:=commandtext+' and flag6 like '''+worksheet1.combobox2.text+'%''';
          if worksheet1.combobox3.text>'' then commandtext:=commandtext+' and flag6='''+worksheet1.combobox3.text+'''';
          if worksheet1.combobox4.text>'' then commandtext:=commandtext+' and dbzs='''+worksheet1.combobox4.text+'''';
          if worksheet1.edit3.text>'' then commandtext:=commandtext+' and acol='''+worksheet1.edit3.text+'''';
          if worksheet1.combobox5.text>'' then commandtext:=commandtext+' and flag2='''+worksheet1.combobox5.text+'''';
    open;
    if not fieldbyname('jhl0').isnull then pjys01.Caption:=formatfloat('0.0',fieldbyname('jhl0').value)
    else pjys01.Caption:='0.0';
  end;
  with worksheet1.ClientDataSet4 do begin
    close;
    params.clear;
          commandtext:='select sum(ysjhl*zjs)/sum(zjs) as jhl1 from tblshedule where yzh=0 and ((fccs like ''%u'') or (fccs like ''%d%'') or (fccs like ''%D'')) and zjs>0 and jhl>0';
          if worksheet1.edit2.text>'' then commandtext:=commandtext+' and upper(pline)='''+worksheet1.edit2.text+'''';
          if worksheet1.edit1.text>'' then commandtext:=commandtext+' and j_no='''+worksheet1.edit1.text+'''';
          if worksheet1.edit4.text>'' then commandtext:=commandtext+' and (tshop like '''+worksheet1.edit4.text+'%'')';
          if worksheet1.edit5.text>'' then commandtext:=commandtext+' and (cstyle like '''+worksheet1.edit5.text+'%'')';
          if worksheet1.edit6.text>'' then commandtext:=commandtext+' and tplant='''+worksheet1.edit6.text+'''';
          if worksheet1.combobox1.text>'' then commandtext:=commandtext+' and grp='''+worksheet1.combobox1.text+'''';
          if worksheet1.edit7.text>'' then commandtext:=commandtext+' and j_no like '''+worksheet1.edit7.text+'%''';
          if worksheet1.combobox2.text>'' then commandtext:=commandtext+' and flag6 like '''+worksheet1.combobox2.text+'%''';
          if worksheet1.combobox3.text>'' then commandtext:=commandtext+' and flag6='''+worksheet1.combobox3.text+'''';
          if worksheet1.combobox4.text>'' then commandtext:=commandtext+' and dbzs='''+worksheet1.combobox4.text+'''';
          if worksheet1.edit3.text>'' then commandtext:=commandtext+' and acol='''+worksheet1.edit3.text+'''';
          if worksheet1.combobox5.text>'' then commandtext:=commandtext+' and flag2='''+worksheet1.combobox5.text+'''';
    open;
    if not fieldbyname('jhl1').isnull then pjjhl.Caption:=formatfloat('0.0',fieldbyname('jhl1').value)
    else pjjhl.Caption:='0.0';
  end;
end;

procedure Tfrmprint5.ppGroupHeaderBand2BeforePrint(Sender: TObject);
begin
  //copy(worksheet1.ClientDataSet1.fieldbyname('plc').value,1,1)
  if (worksheet1.ClientDataSet1.fieldbyname('dta').value='1') or (worksheet1.ClientDataSet1.fieldbyname('dta').value='2') or (worksheet1.ClientDataSet1.fieldbyname('dta').value='3') then begin
    if copy(worksheet1.ClientDataSet1.fieldbyname('lflag').value,1,1)='!' then
      sbtitle001.Caption:='PLC to be acted and DTA to be acted'
    else sbtitle001.Caption:='PLC acted and DTA to be acted';//'SQN''s data* waiting for conduct & not yet ex-fty'
  end else if worksheet1.ClientDataSet1.fieldbyname('dta').value='5' then begin
    if (copy(worksheet1.ClientDataSet1.fieldbyname('lflag').value,1,1)='!') or (copy(worksheet1.ClientDataSet1.fieldbyname('lflag').value,1,1)='^') then
      sbtitle001.Caption:='PLC to be acted and DTA not applicable'
    else sbtitle001.Caption:='PLC acted and DTA not applicable';
  end else begin
    if (copy(worksheet1.ClientDataSet1.fieldbyname('lflag').value,1,1)='!') or (copy(worksheet1.ClientDataSet1.fieldbyname('lflag').value,1,1)='^') then
      sbtitle001.Caption:='PLC to be acted and DTA acted'
    else sbtitle001.caption:='PLC acted and DTA acted';//'SQN''s data* conducted but waiting for ex-fty';
  end;
end;

end.
 
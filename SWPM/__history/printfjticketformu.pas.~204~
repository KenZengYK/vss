procedure Tfrmprintfjticket.ppDetailBand3BeforePrint(Sender: TObject);
begin
  tkt001.Caption:='('+query1.fieldbyname('bundle').asstring+')';
  tkt002.Caption:='('+query1.fieldbyname('bundle').asstring+')';
end;

procedure Tfrmprintfjticket.ppHeaderBand3BeforePrint(Sender: TObject);
begin
  with query2 do begin
    close;
    params.clear;
    params.createparam(ftdatetime,'x1',ptinput);
    commandtext:='select count(distinct psiz) as p from tbl_gxtkt where tm=:x1';
    params[0].asdatetime:=query3.fieldbyname('tm').value;
    open;
    if not fieldbyname('p').isnull then sz001.caption:=fieldbyname('p').asstring;
  end;
end;


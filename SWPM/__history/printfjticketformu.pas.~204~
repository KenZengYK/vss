procedure Tfrmprintfjticket.BitBtn6Click(Sender: TObject);
var
  tm:tdatetime;
begin
  screen.cursor:=crSQLWait;
  tm:=now;
  try
  if worksheet1.tblshedule.fieldbyname('sah_b').value=0 then exit;
  if sEdit1.value>0 then begin
    if label9.caption='QN' then begin
      with query2 do begin
        close;
        params.clear;
        params.createparam(ftstring,'x1',ptinput);
        params.createparam(ftinteger,'x2',ptinput);
        params.createparam(ftinteger,'x3',ptinput);
        params.createparam(ftdatetime,'x4',ptinput);
        commandtext:='execute procedure sp_printfjticket_cut(:x1,:x2,:x3,:x4)';
        params[0].AsString:=worksheet1.tblshedule.fieldbyname('pline').value;
        params[1].asinteger:=worksheet1.tblshedule.fieldbyname('seq').value;
        params[2].asinteger:=sEdit1.Value;
        params[3].asdatetime:=tm;
        execute;
      end;
      with query3 do begin
        close;
        params.clear;
        params.createparam(ftdatetime,'x1',ptinput);
        commandtext:='select distinct tm,pline,j_no,j2_job||''-''||cwo||''-''||rwo||''-''||fccs as qn,cstyle,scqty,acol,psiz,custpo,b_seq,b_qty,b_box,qty,b_box1 '
                    +'from tbl_gxtkt where tm=:x1 order by b_seq';
        params[0].asdatetime:=tm;
        open;
        if not fieldbyname('pline').isnull then begin
          tb001.caption:='[ X ]       B';
          tf001.caption:='[   ]       F';
          ta001.caption:='[   ]       A';
          te001.caption:='[   ]       E';
          ppReport4.print;
          wf001.caption:=formatfloat('#0',worksheet1.tblshedule.fieldbyname('sah_bw').value);
          lb001.caption:='';
          ss001.caption:=formatfloat('0.0000',worksheet1.tblshedule.fieldbyname('sah_ttl').value);
          tb002.caption:='[ X ]       B';
          tf002.caption:='[   ]       F';
          ta002.caption:='[   ]       A';
          te002.caption:='[   ]       E';
          ppReport5.print;
        end;
      end;
    end;
  end;
  finally
    screen.cursor:=crDefault;
  end;
end;

procedure Tfrmprintfjticket.BitBtn7Click(Sender: TObject);
var
  tm:tdatetime;
begin
  screen.cursor:=crSQLWait;
  tm:=now;
  try
  if (worksheet1.tblshedule.fieldbyname('pfsah').value-worksheet1.tblshedule.fieldbyname('sah_b').value<=0) then exit;
  if sEdit1.value>0 then begin
    if label9.caption='QN' then begin
      with query2 do begin
        close;
        params.clear;
        params.createparam(ftstring,'x1',ptinput);
        params.createparam(ftinteger,'x2',ptinput);
        params.createparam(ftinteger,'x3',ptinput);
        params.createparam(ftdatetime,'x4',ptinput);
        commandtext:='execute procedure sp_printfjticket_cut(:x1,:x2,:x3,:x4)';
        params[0].AsString:=worksheet1.tblshedule.fieldbyname('pline').value;
        params[1].asinteger:=worksheet1.tblshedule.fieldbyname('seq').value;
        params[2].asinteger:=sEdit1.Value;
        params[3].asdatetime:=tm;
        execute;
      end;
      with query3 do begin
        close;
        params.clear;
        params.createparam(ftdatetime,'x1',ptinput);
        commandtext:='select distinct tm,pline,j_no,j2_job||''-''||cwo||''-''||rwo||''-''||fccs as qn,cstyle,scqty,acol,psiz,custpo,b_seq,b_qty,b_box,qty,b_box1 '
                    +'from tbl_gxtkt where tm=:x1 order by b_seq';
        params[0].asdatetime:=tm;
        open;
        if not fieldbyname('pline').isnull then begin
          tb001.caption:='[   ]       B';
          tf001.caption:='[ X ]       F';
          ta001.caption:='[   ]       A';
          te001.caption:='[   ]       E';
          ppReport4.print;
          wf001.caption:=formatfloat('#0',worksheet1.tblshedule.fieldbyname('pfrs_a').value-worksheet1.tblshedule.fieldbyname('sah_bw').value);
          lb001.caption:='';
          ss001.caption:=formatfloat('0.0000',worksheet1.tblshedule.fieldbyname('sah_ttl').value);
          tb002.caption:='[   ]       B';
          tf002.caption:='[ X ]       F';
          ta002.caption:='[   ]       A';
          te002.caption:='[   ]       E';
          ppReport5.print;
        end;
      end;
    end;
  end;
  finally
    screen.cursor:=crDefault;
  end;
end;


var
  frmprintfjticket: Tfrmprintfjticket;

implementation

uses mainformu, worksheet, printclticketformu;

{$R *.dfm}

procedure Tfrmprintfjticket.BitBtn3Click(Sender: TObject);
var
  tm:tdatetime;
begin
  screen.cursor:=crSQLWait;
  tm:=now;
  try
  if sEdit1.value>0 then begin
    if label9.caption='QN' then begin
      with query2 do begin
        close;
        params.clear;
        params.createparam(ftstring,'x1',ptinput);
        params.createparam(ftinteger,'x2',ptinput);
        params.createparam(ftinteger,'x3',ptinput);
        params.createparam(ftdatetime,'x4',ptinput);
        commandtext:='execute procedure sp_printfjticket_cut(:x1,:x2,:x3,:x4)';
        params[0].AsString:=worksheet1.tblshedule.fieldbyname('pline').value;
        params[1].asinteger:=worksheet1.tblshedule.fieldbyname('seq').value;
        params[2].asinteger:=sEdit1.Value;
        params[3].asdatetime:=tm;
        execute;
      end;
      with query3 do begin
        close;
        params.clear;
        params.createparam(ftdatetime,'x1',ptinput);
        commandtext:='select distinct tm,pline,j_no,j2_job||''-''||cwo||''-''||rwo||''-''||fccs as qn,cstyle,scqty,acol,psiz,custpo,b_seq,b_qty,b_box,qty,b_box1 '
                    +'from tbl_gxtkt where tm=:x1 order by b_seq';
        params[0].asdatetime:=tm;
        open;
        if not fieldbyname('pline').isnull then begin
          tp001.caption:='[ X ]       Pick';
          t1001.caption:='[   ]       T1 (Received by sewing dept)';
          t2001.caption:='[   ]       T2 (Received by packing dept)';
          ppReport3.print;
        end;
      end;
    end;
  end;
  finally
    screen.cursor:=crDefault;
  end;
end;


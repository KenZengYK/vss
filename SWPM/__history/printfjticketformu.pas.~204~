procedure Tfrmprintfjticket.BitBtn1Click(Sender: TObject);
var
  tm:tdatetime;
begin
  screen.cursor:=crSQLWait;
  tm:=now;
  try
    if sEdit1.value>0 then begin
      if label9.caption='QN' then begin
        with query1 do begin
          close;
          params.clear;
          params.createparam(ftstring,'x1',ptinput);
          params.createparam(ftinteger,'x2',ptinput);
          params.createparam(ftinteger,'x3',ptinput);
          commandtext:='select * from sp_printfjticket(:x1,:x2,:x3) order by bundle';
          params[0].AsString:=worksheet1.tblshedule.fieldbyname('pline').value;
          params[1].asinteger:=worksheet1.tblshedule.fieldbyname('seq').value;
          params[2].asinteger:=sEdit1.Value;
          open;
          if not fieldbyname('cstyle').isnull then begin
            ppReport7.Print;
          end;
        end;

      end else begin
        with query1 do begin
          close;
          params.clear;
          params.createparam(ftstring,'x1',ptinput);
          params.createparam(ftinteger,'x2',ptinput);
          params.createparam(ftinteger,'x3',ptinput);
          commandtext:='select * from sp_printfjticket_smpl(:x1,:x2,:x3) order by bundle';
          params[0].AsString:=worksheet1.tblshedule.fieldbyname('pline').value;
          params[1].asinteger:=worksheet1.tblshedule.fieldbyname('seq').value;
          params[2].asinteger:=sEdit1.Value;
          open;
          if not fieldbyname('cstyle').isnull then begin
            with adoquery1 do begin
              close;
              sql.text:='exec phgdb.dbo.sp_getfjieboo_new1 :x1,:x2,:x3';
              parameters[0].value:=query1.fieldbyname('cstyle').value;
              parameters[1].value:=query1.fieldbyname('iecls').value;
              parameters[2].value:=query1.fieldbyname('fty').value;
              open;
            end;
            ppReport1.Print;
          end;
        end;
      end;
    end;
  {
  if worksheet1.tblshedule.fieldbyname('tplant').value='CL' then begin
    if frmprintclticket=nil then frmprintclticket:=tfrmprintclticket.create(nil);
    frmprintclticket.sEdit1.value:=sEdit1.value;
    frmprintclticket.label9.caption:=label9.caption;
    frmprintclticket.bitbtn1click(self);
  end else begin
    if sEdit1.value>0 then begin
      if label9.caption='QN' then begin
        with query1 do begin
          close;
          params.clear;
          params.createparam(ftstring,'x1',ptinput);
          params.createparam(ftinteger,'x2',ptinput);
          params.createparam(ftinteger,'x3',ptinput);
          commandtext:='select * from sp_printfjticket(:x1,:x2,:x3) order by bundle';
          params[0].AsString:=worksheet1.tblshedule.fieldbyname('pline').value;
          params[1].asinteger:=worksheet1.tblshedule.fieldbyname('seq').value;
          params[2].asinteger:=sEdit1.Value;
          open;
          if not fieldbyname('cstyle').isnull then begin
            with adoquery1 do begin
              close;
              sql.text:='exec phgdb.dbo.sp_getfjieboo_new1 :x1,:x2,:x3';
              parameters[0].value:=query1.fieldbyname('cstyle').value;
              parameters[1].value:=query1.fieldbyname('iecls').value;
              parameters[2].value:=query1.fieldbyname('fty').value;
              open;
            end;
            ppReport1.Print;
          end;
        end;

      end else begin
        with query1 do begin
          close;
          params.clear;
          params.createparam(ftstring,'x1',ptinput);
          params.createparam(ftinteger,'x2',ptinput);
          params.createparam(ftinteger,'x3',ptinput);
          commandtext:='select * from sp_printfjticket_smpl(:x1,:x2,:x3) order by bundle';
          params[0].AsString:=worksheet1.tblshedule.fieldbyname('pline').value;
          params[1].asinteger:=worksheet1.tblshedule.fieldbyname('seq').value;
          params[2].asinteger:=sEdit1.Value;
          open;
          if not fieldbyname('cstyle').isnull then begin
            with adoquery1 do begin
              close;
              sql.text:='exec phgdb.dbo.sp_getfjieboo_new1 :x1,:x2,:x3';
              parameters[0].value:=query1.fieldbyname('cstyle').value;
              parameters[1].value:=query1.fieldbyname('iecls').value;
              parameters[2].value:=query1.fieldbyname('fty').value;
              open;
            end;
            ppReport1.Print;
          end;
        end;
      end;
    end;
  end;
  }
  finally
    screen.cursor:=crDefault;
  end;
end;


      else sb5:='SET';//fieldbyname('sb5').value;
      if pos('-',fieldbyname('sb6').value)>0 then sb6:=sbcode1(copy(fieldbyname('sb6').value,1,3))+chr(13)+chr(20)+copy(fieldbyname('sb6').value,5,3)

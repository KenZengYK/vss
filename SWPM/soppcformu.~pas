unit soppcformu;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, Grids, DBGridEh, ExtCtrls, DB, DBClient, StdCtrls, Buttons;

type
  Tfrmsoppc = class(TForm)
    Panel1: TPanel;
    DBGridEh1: TDBGridEh;
    Query1: TClientDataSet;
    DataSource1: TDataSource;
    BitBtn1: TBitBtn;
    procedure FormClose(Sender: TObject; var Action: TCloseAction);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  frmsoppc: Tfrmsoppc;

implementation

uses mainformu, matmatchu, matchform;

{$R *.dfm}

procedure Tfrmsoppc.FormClose(Sender: TObject; var Action: TCloseAction);
begin
  action:=cafree;
  frmsoppc:=nil;
end;

end.
